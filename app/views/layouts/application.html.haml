<!DOCTYPE html>
%html
  %head
    %title JustintooDotCom
    = stylesheet_link_tag    "application"
    = javascript_include_tag "application"
    = csrf_meta_tags

    :javascript
      $('navbar').scrollspy();


  %body(data-spy="scroll")
    %div(class="navbar navbar-fixed-top")
      %div(class="navbar-inner")
        %div(class="container")
          %a(class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse")
            %span(class="icon-bar")
            %span(class="icon-bar")
            %span(class="icon-bar")
          %a(class="brand" href="#") Project name
          %div(id="navbar" class="nav-collapse")
            %ul(class="nav")
              %li(class="active")
                %a(href="#home") Home
              %li
                %a(href="#contact") Contact
              %li
                %a(href="#about") About
              %li
                %a(href="#end") About

    %div id="home" class="container"

    #home
    - (0...50).each do |i|
      %p= 'Home'

    #contact
    - (0...50).each do |i|
      %p= 'Contact'

    #about
    - (0...50).each do |i|
      %p= 'About'

    #end
    - (0...2).each do |i|
      %p= 'End'

